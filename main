#include "Form.h"
#include<vector>
using namespace std;
int main() {
    vector<Form> students;
    while (true) {
        string n, d;
        double m, e, c, t, s;
        cin >> n;
        if (n == "-1") break;
        cin >> d >> m >> e >> c >> t >> s;
        students.push_back(Form(n, d, m, e, c, t, s));
        students.back().print();
    }
}
